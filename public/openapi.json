    "/api/projects": {
      "get": {
        "summary": "지원사업 필터 및 조회",
        "security": [],
        "parameters": [
          {
            "in": "query",
            "name": "page",
            "schema": {
              "default": 1,
              "type": "integer",
              "minimum": 1,
              "maximum": 9007199254740991
            }
          },
          {
            "in": "query",
            "name": "limit",
            "schema": {
              "default": 10,
              "type": "integer",
              "minimum": 1,
              "maximum": 100
            }
          },
          {
            "in": "query",
            "name": "applicationType",
            "schema": {
              "type": "string",
              "enum": [
                "ALWAYS",
                "FIRST_COME",
                "DEADLINED"
              ]
            }
          },
          {
            "in": "query",
            "name": "serviceType",
            "schema": {
              "type": "string",
              "enum": [
                "STORE_DEMOLITION_SUBSIDY",
                "CLOSURE_SUPPORT_SUBSIDY",
                "CLOSURE_CONSULTING",
                "REEMPLOYMENT_EDUCATION",
                "BUSINESS_EDUCATION"
              ]
            }
          },
          {
            "in": "query",
            "name": "maxAmountRange",
            "schema": {
              "type": "string",
              "enum": [
                "under_100",
                "100_to_300",
                "300_to_500",
                "above_500"
              ]
            }
          },
          {
            "in": "query",
            "name": "isOpen",
            "schema": {
              "type": "boolean"
            }
          },
          {
            "in": "query",
            "name": "appliableOnly",
            "schema": {
              "type": "boolean"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "지원사업 필터 및 조회 성공",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
